{
  "name": "biekerudan/pathfinder",
  "description": "Mapping tool for EVE ONLINE",
  "type": "project",
  "homepage": "https://github.com/biekerudan/pathfinder",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Mark Friedrich",
      "email": "pathfinder@exodus4d.de"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "lock": true,
    "platform": {
      "php": "8.4.13"
    }
  },
  "autoload": {
    "psr-4": {
      "Exodus4D\\Pathfinder\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "readme": "README.md",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/BiekerUdan/pathfinder_esi.git"
    }
  ],
  "require": {
    "php-64bit": ">=8.4",
    "ext-pdo": "*",
    "ext-openssl": "*",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-ctype": "*",
    "ext-gd": "*",
    "bcosca/fatfree-core": "^3.8",
    "ikkez/f3-cortex": "^1.7.8",
    "ikkez/f3-sheet": "0.4.*",
    "xfra35/f3-cron": "1.3.*",
    "monolog/monolog": "^3.0",
    "symfony/mailer": "^7.0",
    "league/html-to-markdown": "^5.0",
    "cache/redis-adapter": "^1.2",
    "cache/filesystem-adapter": "^1.2",
    "cache/array-adapter": "^1.2",
    "cache/void-adapter": "^1.2",
    "cache/namespaced-cache": "^1.2",
    "react/socket": "^1.15",
    "react/promise-stream": "^1.6",
    "clue/ndjson-react": "^1.3",
    "firebase/php-jwt": "^6.10",
    "biekerudan/pathfinder_esi": "dev-master",
    "spiral/roadrunner-worker": "^3.0",
    "spiral/roadrunner-http": "^3.0",
    "nyholm/psr7": "^1.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "vlucas/phpdotenv": "^5.6",
    "phpstan/phpstan": "2.1.x-dev"
  },
  "suggest": {
    "ext-redis": "Redis can be used as cache backend."
  },
  "scripts": {
    "test": "phpunit",
    "phpstan": "phpstan analyse --memory-limit=1G",
    "phpstan-baseline": "phpstan analyse --memory-limit=1G --generate-baseline phpstan-baseline.neon"
  }
}
